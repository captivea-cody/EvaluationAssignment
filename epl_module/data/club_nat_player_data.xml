        </record>


        <!-- Player records -->

        <record id="harry_kane" model="epl.player">
            <field name="name">Harry Kane</field>
            <field name="age">29</field>
            <field name="club_id" eval="club_tottenham_hotspur"/>
            <field name="nat_id" eval="nat_england"/>
            <field name="play_position">cf</field>
            <field name="goals_scored">195</field>
            <field name="apps_made">260</field>
        </record>
        <record id="ben_davies" model="epl.player">
